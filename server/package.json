{
  "name": "touchme-server",
  "version": "1.0.0",
  "description": "Websockets server for desktop app ",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "seed": "ts-node src/seed.ts seed",
    "flush": "ts-node src/seed.ts flush"
  },
  "keywords": [
    "websockets"
  ],
  "author": "James Gates",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^8.11.4",
    "dotenv": "^16.4.5",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "cli-highlight": "^2.1.11",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3",
    "socket.io": "^4.5.1"
  }
}
